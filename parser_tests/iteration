bool fun() 
{


 foreach (item : 1, 2, 3, 2+2, f(), a[20]) 
 {
   a = a + item;
 }


 for (i = 0 :
      i < 100 : 
      i = i + 1) 
 {
       a = a + 1;
 }


 for (i = 0, j = 0 :
      i < 100 : 
      i = i + 1, j = j + 1) 
 {
       a = a + 1;
 }

 for ({i = 0; j = 0;} :
      i < 100 : 
      i = i + 1, j = j + 1) 
 {
       a = a + 1;
 }
 

 while (1 == 1) do {
   a = 1;
 }


 do {
   a = a+1;
 } while(true);

}
